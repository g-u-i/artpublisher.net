{{#each items}}
  <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
    <img height="20" src="assets/images/marker-icon.svg"> <a href='#place:{{slug}}'>{{name}}</a>
  </div>
  {{else}}
  <div class="empty  col-md-offset-3 col-md-6 col-sm-offset-0 col-sm-12  ">
    <h4>Nobody there !</h4>
    <p><em>You should zoom out, select more checkboxes or choose a location.</em></p>
  </div>
{{/each}}
